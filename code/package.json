{
  "name": "feed-the-parrot",
  "version": "1.0.0",
  "description": "RSS feed reader for Alexa",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf ../lambda/!(node_modules)",
    "clean-full": "rimraf ../lambda",
    "compile": "tsc --build tsconfig.json --pretty",
    "build-final": "cpy package.json ../lambda && cd ../lambda/ && npm install --production",
    "test": "echo \"Error: no test specified\" && exit 0",
    "lint-init": "eslint --init",
    "lint-check": "gts check",
    "lint-clean": "gts clean",
    "lint-fix": "gts fix",
    "build": "npm run clean && npm run test && npm run lint-check && npm run compile && npm run build-final",
    "rebuild": "npm run clean-full && npm run test && npm run lint-check && npm run compile && npm run build-final"
  },
  "author": "algono",
  "license": "ISC",
  "dependencies": {
    "ask-sdk-core": "^2.9.0",
    "ask-sdk-model": "^1.29.0",
    "feedparser": "^2.2.10",
    "firebase-admin": "^9.1.1",
    "html-entities": "^1.3.1",
    "i18next": "^19.4.5",
    "node-fetch": "^2.6.0",
    "striptags": "^3.1.1"
  },
  "devDependencies": {
    "@types/feedparser": "^2.2.3",
    "@types/node": "^14.6.3",
    "@types/node-fetch": "^2.5.7",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "ask-sdk-local-debug": "^1.0.0",
    "cpy-cli": "^3.1.1",
    "eslint": "^7.8.1",
    "gts": "^2.0.2",
    "rimraf": "^3.0.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  }
}
