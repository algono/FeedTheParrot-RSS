{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "npm",
			"script": "build",
			"path": "code/",
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"problemMatcher": [
				"$eslint-stylish",
				"$tsc"
			],
			"label": "npm: build - code",
			"detail": "Clean, lint, and build code"
		},
		{
			"type": "npm",
			"script": "build-debug",
			"path": "code/",
			"group": "build",
			"problemMatcher": ["$tsc"],
			"label": "npm: build - debug",
			"detail": "Build code quickly for debugging"
		}
	]
}